<view class="alldata"><an-layer class="vue-ref" vue-id="1" autoClose timer="2" data-ref="anRef" bind:__l="__l"></an-layer><view class="sweep_code"><view class="sweep_code_data"><input placeholder="请输入预约单号,或扫码" data-event-opts="{{[['blur',[['getInfo',['$event']]]],['input',[['__set_model',['','orderNo','$event',[]]]]]]}}" value="{{orderNo}}" bindblur="__e" bindinput="__e"/><button data-event-opts="{{[['tap',[['existFruit',['$event']]]]]}}" bindtap="__e">确定</button><text data-event-opts="{{[['tap',[['sweepCode',['$event']]]]]}}" class="iconfont uni-bold" bindtap="__e"></text></view></view><view class="exist_view"><view class="exist_info"><view><text>预约单号：</text><text>{{orderInfo.fkReserveNo}}</text></view><view><text>收取员工姓名：</text><text>{{orderInfo.name}}</text></view><view><text>收取员工编号：</text><text>{{orderInfo.fkWorkerNo}}</text></view><view><text class="uni-bold">取件码：</text><text class="uni-bold">{{orderInfo.code}}</text></view><view><button data-event-opts="{{[['tap',[['getSackInfo',['$event']]]]]}}" class="uni-bold" bindtap="__e">查看订单详情</button></view></view></view></view>