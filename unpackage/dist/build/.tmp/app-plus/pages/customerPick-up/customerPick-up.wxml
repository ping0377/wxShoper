<view class="alldata"><an-layer class="vue-ref" vue-id="1" autoClose timer="2" data-ref="anRef" bind:__l="__l"></an-layer><view class="sweep_code"><view class="sweep_code_data"><input placeholder="请输入预约单号,或扫码" data-event-opts="{{[['blur',[['getInfo',['$event']]]],['input',[['__set_model',['','orderNo','$event',[]]]]]]}}" value="{{orderNo}}" bindblur="__e" bindinput="__e"/><button data-event-opts="{{[['tap',[['existFruit',['$event']]]]]}}" class="uni-bold" bindtap="__e">订单信息</button><text data-event-opts="{{[['tap',[['sweepCode',['$event']]]]]}}" class="iconfont uni-bold" bindtap="__e"></text></view></view><view class="exist_view"><view class="exist_info"><view><text>预约单号：</text><text>{{orderInfo.fkReserveNo}}</text></view><view><text>客户名称：</text><text>{{orderInfo.Name}}</text></view><view class="uni-bold"><text>取件码：</text><text>{{orderInfo.code}}</text></view><view class="button_view"><button data-event-opts="{{[['tap',[['setOrderInfo',['$event']]]]]}}" class="uni-bold" bindtap="__e">查看订单详情</button><button class="uni-bold" type="primary" data-event-opts="{{[['tap',[['pickUp',['$event']]]]]}}" bindtap="__e">取件</button></view></view></view></view>